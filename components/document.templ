package components

templ Document(content string) {
	<!DOCTYPE html>
	<html lang="en">
		<head>
			<meta charset="UTF-8"/>
			<meta name="viewport" content="width=device-width, initial-scale=1.0"/>
			<title>Document</title>
			<meta content="dark light" name="color-scheme"/>
		</head>
		<body>
			{ content }
		</body>
	</html>
}
